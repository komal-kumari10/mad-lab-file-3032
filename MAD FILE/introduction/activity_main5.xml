<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity5">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        android:background="#E8A3BB"
        tools:layout_editor_absoluteY="1dp">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Introduction"
            android:textAlignment="center"
            android:background="@color/black"
            android:textColor="@color/white"
            android:textSize="40dp"/>

        <TextView
            android:id="@+id/textView12"
            android:layout_width="match_parent"
            android:layout_height="254dp"
            android:layout_marginTop="80dp"
            android:background="#E8A3BB"
            android:text=" Good morning everyone. My name is Komal Kumari. I am from Phagwara. I am pursuing BCA from GNA university. I have 4 members in my family, my mother, my father, my brother and I. My father is a computer operator. My mother is a housewife. I have keen interest in web designing. I usually like to play BGMI in my spare time. The aim of my life is that I want to be a developer. "
            android:textColor="@color/black"
            android:textSize="20dp" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
